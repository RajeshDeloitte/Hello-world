<apex:page id="AppxStoreBldrLayout" name="PrivateExchangeLayout" standardStylesheets="false" sidebar="false">
    <html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
        <head>
            <meta charset="utf-8" />
            <title>{!$Label.asb_Private_AppExchange}</title>
            <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
            <!--  
            <meta name="apple-mobile-web-app-capable" content="yes" />
            -->
            <meta name="description" content="" />
            <meta name="keywords" content="" />
            
            <apex:stylesheet value="{!$Resource.uilib}/css/external/styleExternal.css" />
            <apex:stylesheet value="{!$Resource.uilib}/css/stylePrx.css" />

            <apex:outputPanel rendered="{!!ISPICKVAL($User.UserType,'Standard')}" layout="none">
                <apex:stylesheet value="{!$Resource.uilib}/css/external/custom.css" />
            </apex:outputPanel>

            <link rel="icon" href="{!URLFOR($Resource.SageMarketPlaceFavicon)}" />                    
            <link rel="apple-touch-icon" href="{!$Resource.uilib}/ico/mobile-prx-icon.png" />

            <script>
                var PrX = {}; // global object container
            </script>
            
            <script src="{!$Resource.uilib}/js/external/jquery-min.js" type="text/javascript" ></script>
            <script src="{!$Resource.uilib}/js/external/jquery.debouncedresize.js" type="text/javascript" ></script>
            <script src="{!$Resource.uilib}/js/external/jquery.jscroll.js" type="text/javascript" ></script>
            
            <apex:insert name="head" />
        </head>
        
        <body>
            <div class="prx" id="prx">
                <div class="prx-wrap">                    
                    <apex:outputPanel layout="block" styleClass="prx-wrap-inner">
                        <apex:insert name="content" />
                    </apex:outputPanel>
                </div>
             
                <span class="phone-test" id="phone-test">&#160;</span>
                <span class="small-test" id="small-test">&#160;</span>
                <span class="large-test" id="large-test">&#160;</span>
                
                <apex:outputPanel rendered="{!ISPICKVAL($User.UserType,'Guest')}" layout="none">
                    <div class="custom-footer">
                        <div class="custom-footer-inner">
                            <span class="custom-footer-brand">Copyright Â© {!year(today())}, {!$Label.asb_FooterText}</span>
                            &nbsp;|&nbsp;<a href="http://www.sage.com/company/privacy-policy-and-cookies" class="custom-footer-brand" target="_blank" title="{!$Label.asb_PrivacyPolicy}">{!$Label.asb_PrivacyPolicy}</a>
                            &nbsp;|&nbsp;<a href="http://www.sage.com/company/legal-statement" class="custom-footer-brand" target="_blank" title="{!$Label.asb_TermsOfUse}">{!$Label.asb_TermsOfUse}</a>
                        </div>
                        <span class="custom-footer-disclaimer">{!$Label.asb_Disclaimer}</span>
                    </div>
                </apex:outputPanel>

                <span class="phone-test" id="phone-test">&#160;</span>
                <span class="small-test" id="small-test">&#160;</span>
                <span class="large-test" id="large-test">&#160;</span>
                <span class="phone-test" id="phone-test">&#160;</span>
                <span class="small-test" id="small-test">&#160;</span>
                <span class="large-test" id="large-test">&#160;</span>
                <span class="phone-test" id="phone-test">&#160;</span>
                <span class="small-test" id="small-test">&#160;</span>
                <span class="large-test" id="large-test">&#160;</span>
           </div>
           <br/><br/><br/>
            <script src="{!$Resource.uilib}/js/prxBodyCombined.js" type="text/javascript" ></script>
    
            <script>
                $('#resultsPanel').jscroll({
                    nextSelector: '.jscroll-next:last',
                    loadingHtml: '<small>{!$Label.asb_Loading}</small>'
                });
                PrX.thousandsDelimiter = '{!$Label.asb_thousandsDelimiter}';
                PrX.charsLeftText = '{!$Label.asb_CharactersLeft}';
                PrX.charsOverText = '{!$Label.asb_CharactersTooMany}';
            </script>

            <script>
              (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
              (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
              m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
              })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
            
              ga('create', 'UA-72589854-7', 'auto');
              ga('send', 'pageview');
            
            </script>

        </body>     
    </html>
</apex:page>